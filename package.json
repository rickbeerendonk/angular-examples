{
  "name": "angular-examples",
  "version": "1.0.0",
  "description": "Examples for Angular",
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/angular-examples.git"
  },
  "bugs": {
    "url": "https://github.com/rickbeerendonk/angular-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/angular-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "scripts": {
    "prettier": "prettier --list-different '**/*.{html,js,json,md,ts}'",
    "prettier-fix": "prettier --write '**/*.{html,js,json,md,ts}'",
    "openserver": "node ./server/server.js",
    "start": "npm run openserver -- \".\""
  },
  "dependencies": {
    "@angular/animations": "next",
    "@angular/cdk": "next",
    "@angular/common": "next",
    "@angular/compiler": "next",
    "@angular/core": "next",
    "@angular/forms": "next",
    "@angular/localize": "next",
    "@angular/material": "next",
    "@angular/platform-browser": "next",
    "@angular/platform-browser-dynamic": "next",
    "@angular/platform-server": "next",
    "@angular/pwa": "next",
    "@angular/router": "next",
    "@angular/service-worker": "next",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@nguniversal/express-engine": "next",
    "express": "^4.18.2",
    "ngx-quicklink": "0.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "next",
    "@angular/cli": "next",
    "@angular/compiler-cli": "next",
    "@nguniversal/builders": "next",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.15.3",
    "jasmine-core": "^4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5",
    "husky": "4.3.8",
    "prettier": "2.8.3"
  },
  "overrides": {
    "@ngrx/store": {
      "@angular/core": "next"
    },
    "ngx-quicklink": {
      "@angular/common": "next",
      "@angular/core": "next"
    }
  }
}
