{
  "name": "angular-examples",
  "version": "1.0.0",
  "description": "Examples for Angular",
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/angular-examples.git"
  },
  "bugs": {
    "url": "https://github.com/rickbeerendonk/angular-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/angular-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "scripts": {
    "prettier": "prettier --list-different '**/*.{html,js,json,md,ts}'",
    "prettier-fix": "prettier --write '**/*.{html,js,json,md,ts}'",
    "openserver": "node ./server/server.js",
    "start": "npm run openserver -- \".\""
  },
  "dependencies": {
    "@angular/animations": "latest",
    "@angular/cdk": "latest",
    "@angular/common": "latest",
    "@angular/compiler": "latest",
    "@angular/core": "latest",
    "@angular/forms": "latest",
    "@angular/localize": "latest",
    "@angular/material": "latest",
    "@angular/platform-browser": "latest",
    "@angular/platform-browser-dynamic": "latest",
    "@angular/platform-server": "latest",
    "@angular/pwa": "latest",
    "@angular/router": "latest",
    "@angular/service-worker": "latest",
    "@ngrx/store": "latest",
    "@ngrx/store-devtools": "latest",
    "@nguniversal/express-engine": "latest",
    "express": "^4.18.2",
    "ngx-quicklink": "0.4.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "latest",
    "@angular/cli": "latest",
    "@angular/compiler-cli": "latest",
    "@nguniversal/builders": "latest",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.5",
    "@types/node": "^20.4.10",
    "jasmine-core": "^5.1.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.6",
    "husky": "4.3.8",
    "prettier": "3.0.1"
  },
  "overrides": {
    "@ngrx/store": {
      "@angular/core": "latest"
    },
    "ngx-quicklink": {
      "@angular/common": "latest",
      "@angular/core": "latest"
    }
  }
}
